syntax = "v1"

type (
    SendCaptchaReq {
        PhoneNumber string `form:"phoneNumber"`
    }

    SendCaptchaRep {}
)

@server (
    group: sms
)
service carservice {
    @doc(
		summary: "send captcha for mobile phone number login."
	)
    @handler SendCaptcha
    get /common/sms/sendCaptcha (SendCaptchaReq) returns (SendCaptchaRep)
}