syntax = "v1"

import "sms.api"
import "user.api"

info(
	title: "Carservice"
	author: "hjkl1"
	email: "nil"
	version: "v0.0.1"
)

type (
	PingReq {
		HttpCode int `form:"httpCode, default=200"`
	}
	PingRep {
		Result string `json:"result"`
	}
)

service carservice {
	@doc(
		summary: "check server status."
	)
	@handler Ping
	get /ping (PingReq) returns (PingRep)
}