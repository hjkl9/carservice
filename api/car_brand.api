syntax = "v1"

type (
    CarBrandOptionListItem {
        Id uint `json:"id"`
        Label string `json:"label"`
        Pinyin string `json:"pinyin"`
    }
    
    CarBrandOptionListReq {
        // 可选
        Page int `form:"page,optional"`
    }
    CarBrandOptionListRep {

    }
)

@server (
    group: carbrand
    prefix: /v1
)
service carservice {
    @doc(
        summary: "获取汽车品牌选项列表"
    )
    @handler OptionList
    get /carBrand/optionList returns ([]CarBrandOptionListItem)
}