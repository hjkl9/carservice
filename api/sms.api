syntax = "v1"

type (
    SendCaptchaReq {
        PhoneNumber string `form:"phoneNumber"`
    }

    SendCaptchaRep {}
)

@server (
    group: sms
)
service carservice {
    @doc(
		summary: "为手机号登录发送短信验证码通知"
	)
    @handler SendCaptcha
    get /common/sms/sendCaptcha (SendCaptchaReq) returns (SendCaptchaRep)
}